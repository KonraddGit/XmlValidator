<?xml version="1.0" encoding="utf-8"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:etd="http://crd.gov.pl/xml/schematy/dziedzinowe/mf/2018/08/24/eD/DefinicjeTypy/" xmlns:vzd="http://crd.gov.pl/xml/schematy/dziedzinowe/mf/2019/02/07/eD/VATZD/" targetNamespace="http://crd.gov.pl/xml/schematy/dziedzinowe/mf/2019/02/07/eD/VATZD/" elementFormDefault="qualified" attributeFormDefault="unqualified" xml:lang="pl">
	<xsd:import namespace="http://crd.gov.pl/xml/schematy/dziedzinowe/mf/2018/08/24/eD/DefinicjeTypy/" schemaLocation="http://crd.gov.pl/xml/schematy/dziedzinowe/mf/2018/08/24/eD/DefinicjeTypy/StrukturyDanych_v5-0E.xsd"/>
	<xsd:complexType name="TNaglowek_VAT-ZD">
		<xsd:annotation>
			<xsd:documentation>Nagłówek deklaracji</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="KodFormularza">
				<xsd:complexType>
					<xsd:simpleContent>
						<xsd:extension base="vzd:TKodFormularza_VAT-ZD">
							<xsd:attribute name="kodSystemowy" type="xsd:string" use="required" fixed="VAT-ZD (1)"/>
							<xsd:attribute name="wersjaSchemy" type="xsd:string" use="required" fixed="3-0E"/>
						</xsd:extension>
					</xsd:simpleContent>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="WariantFormularza">
				<xsd:simpleType>
					<xsd:restriction base="xsd:byte">
						<xsd:enumeration value="1"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:simpleType name="TKodFormularza_VAT-ZD">
		<xsd:annotation>
			<xsd:documentation>Symbol wzoru formularza</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="VAT-ZD"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:element name="Wniosek_VAT-ZD">
		<xsd:annotation>
			<xsd:documentation>ZAWIADOMIENIE O SKORYGOWANIU PODSTAWY OPODATKOWANIA ORAZ KWOTY PODATKU NALEŻNEGO</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="Naglowek" type="vzd:TNaglowek_VAT-ZD"/>
				<xsd:element name="PozycjeSzczegolowe">
					<xsd:annotation>
						<xsd:documentation>Dane identyfikacyjne dłużnika oraz informacje dotyczące kwot korekty podstawy opodatkowania oraz podatku należnego</xsd:documentation>
					</xsd:annotation>
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="P_B" minOccurs="0" maxOccurs="unbounded">
								<xsd:complexType>
									<xsd:sequence>
										<xsd:element name="P_BB" type="etd:TZnakowy">
											<xsd:annotation>
												<xsd:documentation>Nazwa podatnika (dłużnika)</xsd:documentation>
											</xsd:annotation>
										</xsd:element>
										<xsd:element name="P_BC" type="etd:TNrNIP">
											<xsd:annotation>
												<xsd:documentation>Identyfikator podatkowy NIP podatnika (dłużnika)</xsd:documentation>
											</xsd:annotation>
										</xsd:element>
										<xsd:element name="P_BD1" type="etd:TZnakowy">
											<xsd:annotation>
												<xsd:documentation>Nr faktury</xsd:documentation>
											</xsd:annotation>
										</xsd:element>
										<xsd:element name="P_BD2">
											<xsd:annotation>
												<xsd:documentation>Data wystawienia faktury</xsd:documentation>
											</xsd:annotation>
											<xsd:simpleType>
												<xsd:restriction base="etd:TData">
													<xsd:minInclusive value="2011-01-01"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
										<xsd:element name="P_BE">
											<xsd:annotation>
												<xsd:documentation>Data upływu terminu płatności określonego w fakturze lub umowie</xsd:documentation>
											</xsd:annotation>
											<xsd:simpleType>
												<xsd:restriction base="etd:TData">
													<xsd:minInclusive value="2011-01-01"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
										<xsd:element name="P_BF" type="etd:TKwota2Nieujemna">
											<xsd:annotation>
												<xsd:documentation>Kwota korekty podstawy opodatkowania</xsd:documentation>
											</xsd:annotation>
										</xsd:element>
										<xsd:element name="P_BG" type="etd:TKwota2Nieujemna">
											<xsd:annotation>
												<xsd:documentation>Kwota korekty podatku należnego</xsd:documentation>
											</xsd:annotation>
										</xsd:element>
									</xsd:sequence>
									<xsd:attribute name="typ" use="required" fixed="G"/>
								</xsd:complexType>
							</xsd:element>
							<xsd:element name="P_10" type="etd:TKwotaCNieujemna">
								<xsd:annotation>
									<xsd:documentation>Ogółem kwota korekty podstawy opodatkowania</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="P_11" type="etd:TKwotaCNieujemna">
								<xsd:annotation>
									<xsd:documentation>Ogółem kwota korekty podatku należnego</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
</xsd:schema>
